{
  "expo": {
    "name": "meepley-app",
    "slug": "meepley-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/branding/logo-compact.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/branding/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/branding/logo-compact.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/branding/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    }
  }
}
