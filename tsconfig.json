{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@navigation/*": ["navigation/*"],
      "@theme/*": ["theme/*"],
      "@screens/*": ["screens/*"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["utils/config/*"],
      "@hooks/*": ["utils/hooks/*"],
      "@ts/*": ["ts/*"]
    }
  }
}
